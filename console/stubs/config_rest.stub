# ===================================
#  Rest Behavior Config
# ===================================

# Allowed Rest Actions
allowedActions:
  index:
      pageSize: 20
  store:
  show:
  update:
  destroy:

# Model Class name
modelClass: {{studly_author}}\{{studly_plugin}}\Models\{{studly_model}}

# transformer Class name
transformerClass: {{studly_author}}\{{studly_plugin}}\Classes\Transformers\{{studly_model}}Transformer

# Verb Method Prefix
prefix: api